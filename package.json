{
  "publisher": "antfu",
  "name": "Intellij-clicks",
  "displayName": "Intellij Clicks",
  "version": "0.2.1",
  "description": "Mimics intellij selection with double clicks",
  "author": "Bill Hedworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bigsy/vscode-intellij-clicks"
  },
  "bugs": {
    "url": "https://github.com/antfu/vscode-smart-clicks/issues"
  },
  "categories": [
    "Other"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "intellijClicks.clicksInterval": {
          "type": "number",
          "default": 600,
          "description": "The interval between clicks in milliseconds."
        },
        "intellijClicks.triggerDelay": {
          "type": "number",
          "default": 150,
          "description": "The delay after triggering the selection. To prevent conflicting with normal selection."
        },
        "intellijClicks.rules": {
          "type": "object",
          "properties": {
            "character-pair": {
              "type": "boolean",
              "default": true,
              "description": "Pair to inner content of paired characters like parentheses, brackets, braces, angle brackets, and quotes.\n\n```js\n▽\n(foo, bar)\n └──────┘\n\n▽\n\"hello world\"\n └──────────┘\n```"
            }
          },
          "description": "Rule toggles"
        }
      }
    }
  },
  "scripts": {
    "build": "node build.js",
    "compile": "npm run build",
    "dev": "node build.js --watch",
    "vscode:prepublish": "npm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^3.0.0",
    "esbuild": "^0.20.2"
  }
}
